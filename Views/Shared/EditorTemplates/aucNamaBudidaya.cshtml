@model string
@(Html.Kendo().AutoCompleteFor(m => m)
        .Placeholder("Pilih Budidaya")
        .DataTextField("Nama")
        .MinLength(2)
        .Filter("contains")
        .DataSource(dataSource =>
        {
            dataSource.Read(read => read.Action("GetBudidayaAuc", "ddl", new { Area = "Referensi" }).Data("filterBudidaya")).ServerFiltering(true);
        })
        .Events(e => { e.Select("aucNamaBudidayaOnSelect"); e.DataBound("aucNamaBudidayaOnDataBound"); })
)






