@model string
@(Html.Kendo().AutoCompleteFor(m=>m)
        .Placeholder("Grade")
        .DataTextField("Nama")
        .MinLength(2)
        .Filter("contains")
        .DataSource(dataSource =>
        {
            dataSource.Read(read => read.Action("GetGrade1", "ddl", new { Area = "Referensi" }).Data("filterGrade")).ServerFiltering(true);
        })
        .Events(e => { e.Select("aucNamaGradeOnSelect"); e.DataBound("aucNamaGradeOnDataBound"); })
)









