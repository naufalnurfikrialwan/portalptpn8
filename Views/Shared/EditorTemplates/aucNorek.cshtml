@model string
@(Html.Kendo().AutoCompleteFor(m=>m)
        .Placeholder("Norek")
        .DataTextField("Norek")
        .MinLength(2)
        .Filter("contains")
        .DataSource(dataSource =>
        {
            dataSource.Read(read => read.Action("GetRekening1", "ddl", new { Area = "Referensi" }).Data("filterRekening")).ServerFiltering(true);
        })
        .Events(e => { e.Select("aucNorekOnSelect"); e.DataBound("aucNorekOnDataBound"); })
)









