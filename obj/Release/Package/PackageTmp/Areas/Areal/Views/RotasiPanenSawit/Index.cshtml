                                                    <td width="50px" style="color:white; background-color: blue"></td>
                                                    <td width="250px" style="vertical-align:middle; padding-left: 10px;"><label>Rotasi Panen Bulan Ini > 4x</label></td>
                                                </tr>
                                                <tr style="height: 30px;">
                                                    <td width="50px" style="color:white; background-color: green"></td>
                                                    <td width="250px" style="vertical-align:middle; padding-left: 10px;"><label>Rotasi Panen Bulan Ini 4x</label></td>
                                                </tr>
                                                <tr style="height: 30px;">
                                                    <td style="color:black; background-color: yellow"></td>
                                                    <td style="vertical-align:middle;padding-left: 10px;"><label>Rotasi Panen Bulan Ini 3x</label></td>
                                                </tr>
                                                <tr style="height: 30px;">
                                                    <td style="color:black; background-color: orange"></td>
                                                    <td style="vertical-align:middle;padding-left: 10px;"><label>Rotasi Panen Bulan Ini 2x</label></td>
                                                </tr>
                                                <tr style="height: 30px;">
                                                    <td style="color:white; background-color: red"></td>
                                                    <td style="vertical-align:middle;padding-left: 10px;"><label>Rotasi Panen Bulan Ini 1x</label></td>
                                                </tr>
                                                <tr style="height: 30px;">
                                                    <td style="color:white; background-color: black"></td>
                                                    <td style="vertical-align:middle;padding-left: 10px;"><label>Rotasi Panen Bulan Ini 0x</label></td>
                                                </tr>

                                            </table>
                                        </div>

                                    </div>);

