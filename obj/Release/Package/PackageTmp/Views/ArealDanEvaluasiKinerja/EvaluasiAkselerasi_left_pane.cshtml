<script>
    function ddlBudidayaOnSelect(e) {
        budidaya = $('#ddlBudidaya').data('kendoDropDownList').value();
        //loadDataKebun = true;
        resetGridData();
    }

    function ddlBudidayaOnChange(e) {
        budidaya = $('#ddlBudidaya').data('kendoDropDownList').value();
        //loadDataKebun = true;
        resetGridData();
    }

    function aucRekeningOnSelect(e) {
        rekening = $('#aucRekening').data('kendoAutoComplete').value();
        if(rekening.indexOf('-')>-1) rekening = rekening.substr(0, rekening.indexOf('-'));
        rekening = rekening.trim();
        resetGridData();
    }

    function aucRekeningOnChange(e) {
        rekening = $('#aucRekening').data('kendoAutoComplete').value();
        if (rekening.indexOf('-') > -1) rekening = rekening.substr(0, rekening.indexOf('-'));
        rekening = rekening.trim();
        resetGridData();
    }

    function chkKebunOnChange(e) {
        var map = $('#map').data('kendoMap');
        if (chkKebun) {
            chkKebun = false;
            map.layers[indexKebun].hide();
            map.layers[indexAfdeling].hide();
            map.layers[indexBlok].hide();
        }
        else {
            chkKebun = true;
            if (activeLayer == "Kebun") {
                map.layers[indexKebun].show();
                map.layers[indexAfdeling].hide();
                map.layers[indexBlok].hide();
            }
            else if (activeLayer == "Afdeling") {
                map.layers[indexKebun].hide();
                map.layers[indexAfdeling].show();
                map.layers[indexBlok].hide();
            }
            else if (activeLayer == "Blok") {
                map.layers[indexKebun].hide();
                map.layers[indexAfdeling].show();
                map.layers[indexBlok].show();
            }
        }
    }

    function chkStreetMapOnChange(e) {
        var map = $('#map').data('kendoMap');
        if (chkStreetMap) {
            chkStreetMap = false;
            map.layers[indexStreetMap].hide();
        }
        else {
            chkStreetMap = true;
            map.layers[indexStreetMap].show();
        }
    }

    function chkKonturOnChange(e) {
        var map = $('#map').data('kendoMap');
        if (chkKontur) {
            chkKontur = false;
            map.layers[indexKontur].hide();
        }
        else {
            chkKontur = true;
            map.layers[indexKontur].show();
        }
    }

    function chkCitraOnChange(e) {
        var map = $('#map').data('kendoMap');
        if (chkCitra) {
            chkCitra = false;
            map.layers[indexCitra].hide();
        }
        else {
            chkCitra = true;
            map.layers[indexCitra].show();
        }
    }

    function filterRekening(e)
    {
        rekening = $('#aucRekening').data('kendoAutoComplete').value();
        if (rekening.indexOf('-') > -1) rekening = rekening.substr(0, rekening.indexOf('-'));
        rekening = rekening.trim();
        return {
            value: rekening,
        };
    }

    function btnProsesOnClick(e)
    {
        var map = $('#map').data('kendoMap');
        if (typeof map != 'undefined') {

            var txtSumberDana = '';
            var txtKegiatan = $('#aucRekening').data('kendoAutoComplete').value().trim();
            var txtBudidaya = $('#ddlBudidaya').data('kendoDropDownList').text();
            var txtBulan = '';
            var txtUnsur = '';
            if ($('#ddlSumberDana').val() == 'RUTIN')
                txtSumberDana = 'Rutin'
            else if ($('#ddlSumberDana').val() == 'AKSELERASI')
                txtSumberDana = 'Akselerasi'
            else
                txtSumberDana = 'Rutin & Akselerasi'

            if (txtKegiatan.indexOf('-') > -1)
                txtKegiatan = txtKegiatan.substring(0, txtKegiatan.indexOf('-')).trim();

            if ($('#BulanAkhir').val() == $('#BulanAwal').val() && $('#TahunAkhir').val() == $('#TahunAwal').val())
                txtBulan = 'Bulan ' + $('#BulanAkhir').data('kendoDropDownList').text() +' '+ $('#TahunAkhir').val()
            else
                txtBulan = 'Bulan ' + $('#BulanAwal').data('kendoDropDownList').text() + ' ' + $('#TahunAwal').val() + ' sd ' +
                $('#BulanAkhir').data('kendoDropDownList').text() + ' ' + $('#TahunAkhir').val();

            if ($('#ddlTransaksi').val()!='SELURUH')
                txtUnsur = ';' + $('#ddlTransaksi').data('kendoDropDownList').text();

            document.getElementById('judulgrid1').innerHTML = 'Penggunaan Dana ' + txtSumberDana +
                ' Rekening ' + txtKegiatan + ' (' + txtBudidaya + txtUnsur + ')';
            document.getElementById('judulgrid2').innerHTML = txtBulan;

            document.getElementById('judulgroup1').innerHTML = $("input[name=perbandingan]:checked").val() + ' Rutin';
            document.getElementById('judulgroup2').innerHTML = $("input[name=perbandingan]:checked").val() + ' Akselerasi';
            
            var chart = $('#chartDetail').data('kendoChart');
            chart.options.title.text = 'Penggunaan Dana ' + txtSumberDana +
                ' Rekening ' + txtKegiatan + ' (' + txtBudidaya + txtUnsur + '). ' + txtBulan;
            chart.refresh();

            if (activeLayer == 'Kebun') {
                readDataKebun().done(function (dta) {
                    dataKebun = [];
                    for (var i = 0; i < dta.length; i++)
                    {
                        dataKebun.push(JSON.stringify(dta[i]));
                    }

                    map.layers[indexKebun].dataSource.read().done(function () {
                        $('#grdDetail').data('kendoGrid').dataSource.read().done(function () {
                            $('#grdLegend').data('kendoGrid').dataSource.read().done(function () {
                                $('#chartDetail').data('kendoChart').dataSource.read();
                            });
                        });
                    });
                });
            }
            else if (activeLayer == 'Afdeling') {
                readDataAfdeling().done(function (dta) {
                    dataAfdeling = [];
                    for (var i = 0; i < dta.length; i++) {
                        dataAfdeling.push(JSON.stringify(dta[i]));
                    }

                    map.layers[indexAfdeling].dataSource.read().done(function () {
                        $('#grdDetail').data('kendoGrid').dataSource.read().done(function () {
                            $('#grdLegend').data('kendoGrid').dataSource.read().done(function () {
                                $('#chartDetail').data('kendoChart').dataSource.read();
                            });

                        });
                    });
                });
            }
            else {
                readDataBlok().done(function (dta) {
                    dataBlok = [];
                    for (var i = 0; i < dta.length; i++) {
                        dataBlok.push(JSON.stringify(dta[i]));
                    }

                    map.layers[indexBlok].dataSource.read().done(function () {
                        $('#grdDetail').data('kendoGrid').dataSource.read().done(function () {
                            $('#grdLegend').data('kendoGrid').dataSource.read().done(function () {
                                $('#chartDetail').data('kendoChart').dataSource.read();
                            });

                        });
                    });
                });
            }
        }
    }

    function readDataKebun()
    {
        var url = '../ArealDanEvaluasiKinerja/readData';
        var _sumber = $('#ddlSumberDana').val();
        var _blnAwal = $('#BulanAwal').val();
        var _blnAkhir = $('#BulanAkhir').val();
        var _thnAwal = $('#TahunAwal').val();
        var _thnAkhir = $('#TahunAkhir').val();
        var _unsur = $('#ddlTransaksi').data('kendoDropDownList').value();
        var _scriptSQL = "use ArealEF exec PetaView '???','Kebun'," + _blnAwal + "," + _thnAwal + "," +
                    _blnAkhir + "," + _thnAkhir + ",'" + budidaya + "','" +
                    rekening + "','" + _unsur + "','" + $("input[name=perbandingan]:checked").val() + "','SBI'";

        return $.ajax({
            url: url,
            data: {
                Id: lokasiID,
                layer: "Kebun",
                scriptSQL: _scriptSQL,
                _budidaya: budidaya,
                _menuId: menuID
            }, //parameters go here in object literal form
            type: 'POST',
            datatype: 'json'
        });
    }

    function readDataAfdeling()
    {
        var url = '../ArealDanEvaluasiKinerja/readData';
        var _sumber = $('#ddlSumberDana').val();
        var _blnAwal = $('#BulanAwal').val();
        var _blnAkhir = $('#BulanAkhir').val();
        var _thnAwal = $('#TahunAwal').val();
        var _thnAkhir = $('#TahunAkhir').val();
        var _unsur = $('#ddlTransaksi').data('kendoDropDownList').value();
        var _scriptSQL = "use ArealEF exec PetaView '???','Afdeling'," + _blnAwal + "," + _thnAwal + "," +
                    _blnAkhir + "," + _thnAkhir + ",'" + budidaya + "','" +
                    rekening + "','" + _unsur + "','" + $("input[name=perbandingan]:checked").val() + "','SBI'";

        return $.ajax({
            url: url,
            data: {
                Id: lokasiKebunID,
                layer: "Afdeling",
                scriptSQL: _scriptSQL,
                _budidaya: budidaya,
                _menuId: menuID
            }, //parameters go here in object literal form
            type: 'POST',
            datatype: 'json'
        });
    }

    function readDataBlok()
    {
