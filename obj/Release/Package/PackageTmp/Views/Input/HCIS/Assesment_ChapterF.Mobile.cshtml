                                    //src1 = "/Content/Images/Assesment/SoalTipeE_" + noUrut.ToString() + ".jpg";
                                    src1 = "/Content/Images/Assesment/" + pertanyaanDetail.FirstOrDefault().FileFoto.ToString();

