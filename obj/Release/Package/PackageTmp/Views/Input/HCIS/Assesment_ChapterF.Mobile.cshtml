                                                    <input class="largerRadio" type="radio" name="F_@noUrut" value="@mp2.NoUrut" onclick="jawabanOnClick('F_@noUrut','@(mp2.NoUrut)')" /><span class="jawaban" onclick="pertanyaanOnClick('E_@noUrut','@(mp2.NoUrut - 1)')">(@Convert.ToChar(96 + mp2.NoUrut))</span>
                                                </td>
                                                <td>
                                                    <img class="jawaban" onclick="pertanyaanOnClick('F_@noUrut','@(mp2.NoUrut - 1)')" src="@src2" />
                                                </td>
                                            </tr>
                                        }
                                    </table>
                                    mdlLoadPerChapter.JumlahLoadChapterF = mdlLoadPerChapter.JumlahLoadChapterF + 1;
                                    HttpContext.Current.Application["Assesment" + (string)HttpContext.Current.User.Identity.Name] = mdlLoadPerChapter;

