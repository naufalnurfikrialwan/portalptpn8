                                        if ((nI + 1) % 2 != 0) {
                                            <tr width="100%">
                                                <td width="50%">
                                                    <input class="largerRadio" type="radio" name="H_@noUrut" value="@(nI + 1)" onclick="jawabanOnClick('H_@noUrut','@(nI + 1)')" /><span class="jawaban"  style="font-size: x-large;" onclick="pertanyaanOnClick('H_@noUrut','@nI')">(@Convert.ToChar(97 + @nI)) @nI</span>
                                                </td>
                                                <td width="50%">
                                                    <input class="largerRadio" type="radio" name="H_@noUrut" value="@(nI + 1 + 1)" onclick="jawabanOnClick('H_@noUrut','@(nI + 1 + 1)')" /><span class="jawaban" style="font-size: x-large;" onclick="pertanyaanOnClick('H_@noUrut','@(nI + 1)')">(@Convert.ToChar(97 + @nI + 1)) @(nI + 1)</span>
                                                </td>
                                            </tr>
                                        }
                                    }
                                </table>
                                <br />


                                @if (noUrut < jumlahPertanyaan)
