                <div id="TestChapterC" style="display: none;">
                    @{
                        Guid paketId = Ptpn8.Models.ModalManusia.CRUDJadwalNIKBOM4dan5.CRUD.GetRecord(HttpContext.Current.User.Identity.Name).PaketId;
                        Guid tipeSoalId = Ptpn8.Models.ModalManusia.CRUDTipeSoalBOM4dan5.CRUD.GetRecord("Tipe C").TipeSoalId;
                        int jumlahPertanyaan = Ptpn8.Models.ModalManusia.CRUDTipeSoalBOM4dan5.CRUD.GetRecord("Tipe C").JumlahPertanyaan;
                        Ptpn8.Models.ModalManusia.PaketDetailBOM4dan5 paketDetail;
                        Ptpn8.Models.ModalManusia.PertanyaanBOM4dan5 pertanyaan;
                        List<Ptpn8.Models.ModalManusia.PertanyaanDetailBOM4dan5> pertanyaanDetail;
                        List<Ptpn8.Models.ModalManusia.JawabanBOM4dan5> jawaban;
                        string src1;
                        string src2;
                        Ptpn8.Models.ModalManusia.View_LoadSoalBOM4dan5 mdlLoadPerChapter = (Ptpn8.Models.ModalManusia.View_LoadSoalBOM4dan5)HttpContext.Current.Application["Assesment" + (string)HttpContext.Current.User.Identity.Name];
                        if (mdlLoadPerChapter == null)
                        {
                            mdlLoadPerChapter = new Ptpn8.Models.ModalManusia.View_LoadSoalBOM4dan5();
                        }
                        <div style="border: 3px solid black; padding: 10px;">
                            <label style="font-size: xx-large; font-weight: 1000; text-align: center;">SOAL Chapter C</label>
                            <label style="font-size: x-large; font-weight: 600;">INSTRUKSI:</label>
                            <label id="instruksiE2" style="font-size: x-large; font-weight: 600;margin: 20px 20px 20px 20px">@instruksi</label>
                            <label style="font-size: x-large; margin: 20px 20px 20px 20px">Pilih pada salah satu jawaban yang menurut anda benar/paling tepat.Anda dapat mengerjakan soal yang menurut anda lebih mudah terlebih dahulu</label>
                            <label style="font-size: x-large; margin: 20px 20px 20px 20px">Apabila sudah selesai Click <span style="color: red">"Lanjut ke Chapter D"</span></label>
                        </div>
                        <div id="E">
                            @{
                                mdlLoadPerChapter.JumlahLoadChapterC = 0;
                            }
                            @for (int noUrut = 1; noUrut <= jumlahPertanyaan; noUrut++)
                            {
                                paketDetail = Ptpn8.Models.ModalManusia.CRUDPaketDetailBOM4dan5.CRUD.GetRecord(paketId, tipeSoalId, noUrut);
                                pertanyaan = Ptpn8.Models.ModalManusia.CRUDPertanyaanBOM4dan5.CRUD.GetRecord(tipeSoalId, noUrut);
                                pertanyaanDetail = Ptpn8.Models.ModalManusia.CRUDPertanyaanDetailBOM4dan5.CRUD.GetRecord(paketDetail.PertanyaanId, paketId, noUrut);
                                jawaban = Ptpn8.Models.ModalManusia.CRUDJawabanBOM4dan5.CRUD.GetRecord(paketDetail.PertanyaanId, paketId, noUrut);

                                //src1 = "/Content/Images/Assesment/SoalTipeC_" + noUrut.ToString() + ".jpg";
                                src1 = "/Content/Images/Assesment/BOM4dan5/" + pertanyaanDetail.FirstOrDefault().FileFoto.ToString();
                                <table class="multiplechoice" style="background-color: white; border-color: gray;" width="50%">
                                    <tr>
                                        @foreach (var mp1 in pertanyaanDetail)
                                        {
                                            <td width="10%"><label>(@noUrut)</label></td>
                                            <td width="10%"></td>
                                            <td><img src="@src1" /></td>
                                        }
                                    </tr>

                                    @foreach (var mp2 in jawaban)
                                    {
                                        //src2 = "/Content/Images/Assesment/JawabTipeC_" + noUrut.ToString() + Convert.ToChar(96 + mp2.NoUrut) + ".jpg";
                                        src2 = "/Content/Images/Assesment/BOM4dan5/" + mp2.FileFoto;

                                        <tr>
                                            <td></td>
                                            <td>
                                                <label id="C1_@noUrut" style="display: none;">@mp2.PertanyaanId</label>
                                                <input class="largerRadio" type="radio" name="C_@noUrut" value="@mp2.NoUrut" onclick="jawabanOnClick('C_@noUrut','@(mp2.NoUrut)')" /><span class="jawaban" onclick="pertanyaanOnClick('C_@noUrut','@(mp2.NoUrut - 1)')">(@Convert.ToChar(96 + mp2.NoUrut))</span>
                                            </td>
                                            <td><img class="jawaban" onclick="pertanyaanOnClick('C_@noUrut','@(mp2.NoUrut - 1)')" src="@src2" /></td>
                                        </tr>
                                    }
                                </table>
                                mdlLoadPerChapter.JumlahLoadChapterC = mdlLoadPerChapter.JumlahLoadChapterC + 1;
                                HttpContext.Current.Application["Assesment" + (string)HttpContext.Current.User.Identity.Name] = mdlLoadPerChapter;
                            }

                            <table width="100%">
                                <tr>
                                    <td align="center">
                                        <button type="button" id="btnC" onclick="btnOnClick('Tipe C')" class="btn-big" style="font-size: x-large; height: 60px; width: 250px;">Lanjut ke Chapter D</button>
                                    </td>
                                </tr>
                            </table>
                        </div>
                    }
