                        </tr>
                    </table>
                    <table width="100%">
                        <tr>
                            <td align="center">
                                <button type="button" id="btnMulaiTestChapterG" onclick="btnMulaiTestOnClick('Tipe G')" class="btn-big " style="height: 60px; width: 250px;">Mulai Test Chapter G</button>
                            </td>
                        </tr>
                    </table>
                </div>
                <div id="TestChapterG" style="display: none;">
                    @{
                        Guid paketId = Ptpn8.Models.ModalManusia.CRUDJadwalNIKBOM4dan5.CRUD.GetRecord(HttpContext.Current.User.Identity.Name).PaketId;
                        Ptpn8.Models.ModalManusia.PaketDetailBOM4dan5 paketDetail;
                        Guid tipeSoalId = Ptpn8.Models.ModalManusia.CRUDTipeSoalBOM4dan5.CRUD.GetRecord("Tipe G").TipeSoalId;
                        int jumlahPertanyaan = Ptpn8.Models.ModalManusia.CRUDTipeSoalBOM4dan5.CRUD.GetRecord("Tipe G").JumlahPertanyaan;
                        Ptpn8.Models.ModalManusia.PertanyaanBOM4dan5 pertanyaan;
                        List<Ptpn8.Models.ModalManusia.PertanyaanDetailBOM4dan5> pertanyaanDetail;
                        List<Ptpn8.Models.ModalManusia.JawabanBOM4dan5> jawaban;
                        Ptpn8.Models.ModalManusia.View_LoadSoalBOM4dan5 mdlLoadPerChapter = (Ptpn8.Models.ModalManusia.View_LoadSoalBOM4dan5)HttpContext.Current.Application["Assesment" + (string)HttpContext.Current.User.Identity.Name];
                        if (mdlLoadPerChapter == null)
                        {
                            mdlLoadPerChapter = new Ptpn8.Models.ModalManusia.View_LoadSoalBOM4dan5();
                        }
                        @*<div style="border: 3px solid black; padding: 10px;">*@
                        <label style="font-size: large; font-weight: 1000; text-align: center;">SOAL Chapter G</label>
                        <label style="font-size: large; font-weight: 600;">INSTRUKSI:</label>
                        <label id="instruksiJ2" style="font-size: large; font-weight: 600;margin: 5px 5px 5px 5px">@instruksi</label>
                        <label style="font-size: large; margin: 5px 5px 5px 5px">Apabila anda ingin melanjutkan ke soal berikutnya, Click <span style="color: red">"Soal Chapter G Berikutnya"</span> atau <span style="color: red">"Lanjut ke Chapter H"</span></label>
                        @*</div>*@
                        mdlLoadPerChapter.JumlahLoadChapterG = 0;
                        for (int noUrut = 1; noUrut <= jumlahPertanyaan; noUrut++)
                        {
                            paketDetail = Ptpn8.Models.ModalManusia.CRUDPaketDetailBOM4dan5.CRUD.GetRecord(paketId, tipeSoalId, noUrut);
                            pertanyaan = Ptpn8.Models.ModalManusia.CRUDPertanyaanBOM4dan5.CRUD.GetRecord(tipeSoalId, noUrut);
                            pertanyaanDetail = Ptpn8.Models.ModalManusia.CRUDPertanyaanDetailBOM4dan5.CRUD.GetRecord(paketDetail.PertanyaanId);
                            jawaban = Ptpn8.Models.ModalManusia.CRUDJawabanBOM4dan5.CRUD.GetRecord(paketDetail.PertanyaanId);

                            <div id="G_@noUrut.ToString()" style="display: none;">
                                <h2>Soal No @noUrut</h2>
                                <table width="100%" style="text-align: center; color: black; font-size: large; font-weight: 500;">
                                    <tr>
                                        <td width="10%" style="background-color: yellow">P</td>
                                        <td style="background-color: darkcyan">GAMBARAN DIRI</td>
                                        <td width="10%" style="background-color: red">K</td>
                                    </tr>
                                    @foreach (var mp1 in jawaban)
                                    {
                                        <tr>
                                            <td>
                                                <label id="G1A_@noUrut" style="display: none;">@mp1.PertanyaanId</label>
                                                <input class="largerRadio" type="radio" name="G1_@noUrut" value="@mp1.NoUrut" onclick="jawabanOnClick('G1_@noUrut','@(mp1.NoUrut)','G2_@noUrut','@(mp1.NoUrut)')" />
                                            </td>
                                            <td style="color: black;">@(mp1.JawabanTeks.ToUpper())</td>
                                            <td>
                                                <label id="G2A_@noUrut" style="display: none;">@mp1.PertanyaanId</label>
                                                <input class="largerRadio" type="radio" name="G2_@noUrut" value="@mp1.NoUrut" onclick="jawabanOnClick('G1_@noUrut','@(mp1.NoUrut)','G2_@noUrut','@(mp1.NoUrut)')" />
                                            </td>
                                        </tr>
                                    }
                                </table>
                                <br />
                                @if (noUrut < jumlahPertanyaan)
                                {
                                    <table width="100%">
                                        <tr>
                                            <td align="center">
                                                <button type="button" id="btnG_@noUrut" onclick="btnOnClick1('Tipe G')" class="btn-big" style="height: 60px; width: 500px;">Soal Chapter G Berikutnya</button>
                                            </td>
                                        </tr>
                                    </table>
                                }
                                else
                                {
                                    <table width="100%">
                                        <tr>
                                            <td align="center">
                                                <button type="button" id="btnG_@noUrut" onclick="btnOnClick('Tipe G')" class="btn-big" style="height: 60px; width: 250px;">Lanjut ke Chapter H</button>
                                            </td>
                                        </tr>
                                    </table>
                                }
                                <br />
                            </div>
                            mdlLoadPerChapter.JumlahLoadChapterG = mdlLoadPerChapter.JumlahLoadChapterG + 1;
                            HttpContext.Current.Application["Assesment" + (string)HttpContext.Current.User.Identity.Name] = mdlLoadPerChapter;

                        }
                    }

                </div>
            </div>
        </td>
    </tr>
</table>

