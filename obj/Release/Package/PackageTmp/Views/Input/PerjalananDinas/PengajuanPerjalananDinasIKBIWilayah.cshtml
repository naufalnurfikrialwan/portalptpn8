    <script>
        var nama;
        var menuId = '36c558f7-c158-4219-b5c9-897e062ff4c7';
        function btnProsesOnClick() {

            InsertGridFrom().done(function (results) {
                if (results) {
                    var grid = $('#grdDetailNik2').data('kendoGrid');
                    var dataSource = new kendo.data.DataSource({ data: results });
                    dataSource.read();
                    grid.setDataSource(dataSource);
                    grid.refresh();
                }
            });

        }

        function InsertGridFrom() {
            var grd = $('#grdDetailNik2').data('kendoGrid');
            var url = '/Input/GetDataFrom';
            return $.ajax({
                url: url,
                //contentType: 'application/json; charset=utf-8',
                data: {
                    strClassView: "Ptpn8.PerjalananDinas.Models.NIK",
                    scriptSQL: " Select Distinct NAMA,REG from [SPDK_KanpusEF].dbo.Ref_Dik where Nama like '%" + $('#aucNama2').val() + "%' and Kd_kbn ='" + $('#Kd_Kbn').val() + "'" +
                                " Union Select Distinct NAMA,REG from [SPDK_KanpusEF].dbo.Ref_DikKLM where Nama like '%" + $('#aucNama2').val() + "%' and Kd_kbn ='" + $('#Kd_Kbn').val() + "'",
                    _menuId: menuId
                },
                type: 'POST',
                datatype: 'json'
            });
        }

        function btnPilihOnClick(e) {
            e.preventDefault();
            var gridPopUp = this;
            var row = $(e.currentTarget).closest("tr");

            var data = gridPopUp.dataItem(row);
            var nik = data.REG;

            var grid = $('#grdDetail').data('kendoGrid');
            var dataItem = typeof (grid.dataSource.get(dtl_PermintaanPerjalananDinasIKBIId)) == "undefined" ? grid.dataSource.getByUid(dtl_PermintaanPerjalananDinasIKBIId) : grid.dataSource.get(dtl_PermintaanPerjalananDinasIKBIId);
            dataItem.NIK = nik;
            $('#NIK').val(nik);
            grid.refresh();
            ceknik.close();
        }
    </script>
