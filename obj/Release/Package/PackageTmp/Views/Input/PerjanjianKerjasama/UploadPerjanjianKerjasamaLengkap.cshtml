                                        Text = "KSO",
                                        Value = "KSO"
                                    },
                                    new SelectListItem() {
                                        Text = "KSU",
                                        Value = "KSU"
                                    },
                                    new SelectListItem() {
                                        Text = "PINJAM PAKAI",
                                        Value = "PINJAM PAKAI"
                                    },
                                    new SelectListItem() {
                                        Text = "PMDK",
                                        Value = "PMDK"
                                    },
                                    new SelectListItem() {
                                        Text = "KONTRAK",
                                        Value = "KONTRAK"
                                    },
                                    new SelectListItem() {
                                        Text = "MAKLOON",
                                        Value = "MAKLOON"
                                    },
                                    new SelectListItem() {
                                        Text = "RISET",
                                        Value = "RISET"
