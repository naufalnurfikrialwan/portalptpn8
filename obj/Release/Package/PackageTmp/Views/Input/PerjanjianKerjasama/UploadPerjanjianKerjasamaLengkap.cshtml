                                        Value = "PERTANIAN/PERKEBUNAN"
                                    },
                                    new SelectListItem() {
                                        Text = "AGROWISATA",
                                        Value = "AGROWISATA"
                                    },
                                    new SelectListItem() {
                                        Text = "OPTIMALISASI LAHAN",
                                        Value = "OPTIMALISASI LAHAN"
                                    },
                                    new SelectListItem() {
                                        Text = "PERTAMBANGAN",
                                        Value = "PERTAMBANGAN"
                                    },
                                    new SelectListItem() {
                                        Text = "PERIKANAN",
                                        Value = "PERIKANAN"
                                    },
                                    new SelectListItem() {
                                        Text = "PROPERTI",
                                        Value = "PROPERTI"
                                    },
                                    new SelectListItem() {
                                        Text = "ENERGI",
                                        Value = "ENERGI"
                                    },
                                    new SelectListItem() {
                                        Text = "LAIN-LAIN",
